{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@list-app/api/lists/feature": ["libs/api/lists/feature/src/index.ts"],
      "@list-app/api/public/feature": ["libs/api/public/feature/src/index.ts"],
      "@list-app/api/shared/data-access": [
        "libs/api/shared/data-access/src/index.ts"
      ],
      "@list-app/api/shares/feature": ["libs/api/shares/feature/src/index.ts"],
      "@list-app/fe-web/lists/feature": [
        "libs/fe-web/lists/feature/src/index.ts"
      ],
      "@list-app/fe-web/public/feature": [
        "libs/fe-web/public/feature/src/index.ts"
      ],
      "@list-app/fe-web/shared/ui": ["libs/fe-web/shared/ui/src/index.ts"],
      "@list-app/fe-web/shares/feature": [
        "libs/fe-web/shares/feature/src/index.ts"
      ],
      "@list-app/shared/api-interfaces": [
        "libs/shared/api-interfaces/src/index.ts"
      ],
      "@list-app/shared/frontend/data-access": [
        "libs/shared/frontend/data-access/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
